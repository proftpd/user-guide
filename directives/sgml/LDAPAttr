<refnamediv>
<refname>LDAPAttr</refname>
<refpurpose>Map LDAP Attributes to something non standard</refpurpose>
</refnamediv>

<refsynopsisdiv>
<cmdsynopsis>
<command>LDAPAttr</command>
<arg choice="opt">
  <option>uid</option>
  <option>uidNumber</option>
  <option>gidNumber</option>
  <option>homeDirectory</option>
  <option>userPassword</option>
  <option>loginShell</option>
  <option>cn</option>
  <option>memberUid</option>
  <option>ftpQuota</option>
</arg>
<arg choice="opt">
  <option>"NewAttribute"</option>
</arg>
</cmdsynopsis>
 
